{
  "name": "coalesce-vue",
  "version": "2.1.0-dev-002",
  "description": "Coalesce core scripts for Vue.js",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "repository": "https://github.com/IntelliTect/Coalesce",
  "author": "IntelliTect",
  "license": "Apache-2.0",
  "private": false,
  "dependencies": {
    "@types/lodash-es": "^4.17.0",
    "@types/qs": "^6.5.1",
    "axios": "^0.18.0",
    "date-fns": "2.0.0-alpha.7",
    "lodash-es": "^4.17.5",
    "qs": "^6.5.1",
    "vue": "^2.5.13"
  },
  "devDependencies": {
    "@types/jest": "^23.1.1",
    "jest": "^23.1.0",
    "ts-jest": "^22.4.6",
    "typescript": "2.9.2"
  },
  "scripts": {
    "build": "tsc -p ./tsconfig.build.json",
    "prepack": "tsc -p ./tsconfig.build.json",
    "//": "No coverage by default because it breaks line numbers in error messages. `yarn test --coverage` to run it.",
    "test": "jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**",
      "!src/index.ts"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(.*\\.)?(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
