{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "allowSyntheticDefaultImports": true,
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "experimentalDecorators": true,
    "lib": ["es2015", "es2017", "dom"]
  },
  "exclude": [
      "bin",
      "node_modules"
  ]
}
